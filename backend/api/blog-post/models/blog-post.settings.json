{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "name": "blogPost"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "publishDate": {
      "type": "date"
    },
    "post": {
      "type": "richtext",
      "required": true,
      "unique": true
    },
    "blogImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    }
  }
}
